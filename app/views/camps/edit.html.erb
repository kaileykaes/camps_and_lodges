<%= form_with url: "/camps/#{@camp.id}", method: :patch, local: true do |form| %>
<%= form.label :name, 'Name' %>
<%= form.text_field :name %>
<%= form.label :campground_number, 'Campground Number' %>
<%= form.text_field :campground_number %>
<%= form.label :vacancy, 'Vacancy' %>
<%= form.text_field :vacancy %>
<%= form.submit 'Update Camp' %>
<% end %>